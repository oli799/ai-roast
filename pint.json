{
    "preset": "laravel",
    "rules": {
        "array_push": true,
        "backtick_to_shell_exec": true,
        "ordered_traits": true,
        "not_operator_with_successor_space": true,
        "align_multiline_comment": {
            "comment_type": "all_multiline"
        },
        "array_syntax": {
            "syntax": "short"
        },
        "cast_spaces": {
            "space": "single"
        }
    }
}
